<form>
    <div class="form-group">
        <label>Name</label>
        {{input type="text" class="form-control" value=name placeholder = "Enter your name"}}
    </div>
    <div class="form-group">
        <label>Address</label>
        {{textarea type="text" class="form-control" value=address placeholder = "Enter your Address"}}
    </div>
    <div class="form-group">
        <label>Due Date</label>
        {{input type="date" class="form-control" value=date}}
    </div>
    <div class="form-group">
        <label>fees</label>
        {{input type="text" class="form-control" value=fees placeholder = "Enter fees"}}
    </div>
    <button {{action 'addInfo'}}type="button" class="btn btn-primary">Submit !!</button>
</form>
